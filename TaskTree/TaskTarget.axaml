<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="300"
			 HorizontalAlignment="Left" VerticalAlignment="Center"
             x:Class="TaskTree.TaskTarget"
			 x:Name="TaskTargetTag"
			 Loaded="TaskTargetTag_Load"
			 Margin="5">
	<StackPanel Orientation="Horizontal">
		<TextBlock x:Name="Name1" Margin="0,0,5,0"/>
		<HyperlinkButton x:Name="Taskbool1" Content="完成" Foreground="Green" Click="Click_Submit" Margin="0,0,5,0"/>
		<HyperlinkButton x:Name="Task2" IsVisible="False" Click="Click_Del"/>
	</StackPanel>
</UserControl>
